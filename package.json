    "typescript": "~5.7.3",
    "vite": "^6.1.0"
  },
  "dependencies": {
    "@adonisjs/ally": "^5.1.1",
    "@adonisjs/auth": "^9.4.0",
    "@adonisjs/cache": "^1.1.3",
