    "@vinejs/vine": "^3.0.0",
    "better-sqlite3": "^11.9.1",
    "edge.js": "^6.2.1",
    "luxon": "^3.6.1",
    "pino-http": "^10.4.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reflect-metadata": "^0.2.2",
    "tailwindcss": "^4.1.3",
